{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "babenkoivan/scout-elasticsearch-driver": "^4.2",
        "backpack/crud": "4.1.*",
        "backpack/filemanager": "^1.0",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^1.0",
        "fzaninotto/faker": "^1.9",
        "laravel/framework": "^7.0",
        "laravel/helpers": "^1.2",
        "laravel/horizon": "^4.3",
        "laravel/scout": "^8.0",
        "laravel/telescope": "^3.3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "lorisleiva/laravel-deployer": "^0.3.0",
        "mcamara/laravel-localization": "^1.5",
        "spatie/laravel-medialibrary": "8.0.0",
        "spatie/laravel-sluggable": "^2.2",
        "spatie/laravel-translatable": "^4.3",
        "spatie/temporary-directory": "^1.2"
    },
    "require-dev": {
        "backpack/generators": "^3.1",
        "facade/ignition": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "laracasts/generators": "^1.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.1",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/i18n.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-install-cmd": [
            "@php artisan backpack:install",
            "@php artisan elfinder:publish",
            "@php artisan horizon:publish"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
